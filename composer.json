{
    "name": "tituspijean/flarum-ext-auth-ldap",
    "description": "Allow users to log in through a LDAP server.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "TitusPiJean",
            "email": "tituspijean@outlook.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TitusPiJean/flarum-ext-auth-ldap/issues",
        "source": "https://github.com/TitusPiJean/flarum-ext-auth-ldap"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6",
        "adldap/adldap": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\Auth\\LDAP\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "LDAP Login",
            "icon": {
                "name": "lock",
                "backgroundColor": "#b15959",
                "color": "#fff"
            }
        }
    }
}
